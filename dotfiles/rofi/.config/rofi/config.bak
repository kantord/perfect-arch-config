rofi.modi:                           window,run,ssh
rofi.width:                          80
rofi.lines:                          8
rofi.columns:                        2
rofi.font:                           Source Code Pro 9
rofi.color-normal:                   argb:000B1F21, #ffffd3, argb:000B1F21, argb:55af4b24, #ffffd3
rofi.color-urgent:                   argb:000B1F21, #d6b48d, argb:000B1F21, #d6b48d, #ffffd3
rofi.color-active:                   argb:000B1F21, #d5351c, argb:000B1F21, #d5351c, #0B1F21
rofi.color-window:                   argb:cc0B1F21, #af4b24, #783319
rofi.bw:                             0
rofi.eh:                             1
rofi.location:                       0
rofi.padding:                        30
rofi.yoffset:                        0
rofi.xoffset:                        0
rofi.fixed-num-lines:                true
rofi.terminal:                       rofi-sensible-terminal
rofi.ssh-client:                     ssh
rofi.ssh-command:                    {terminal} -e {ssh-client} {host}
rofi.run-command:                    {cmd}
rofi.parse-hosts:                    true
rofi.matching:                       normal
rofi.separator-style:                none
rofi.scrollbar-width:                0
rofi.kb-mode-next:                   Shift+Right,Control+Tab,Alt+l
rofi.kb-mode-previous:               Shift+Left,Control+Shift+Tab,Alt+h
rofi.kb-row-up:                      Up,Control+p,Shift+Tab,Shift+ISO_Left_Tab,Alt+k
rofi.kb-row-down:                    Down,Control+n,Alt+j
rofi.show-icons: 		     true
rofi.hide-scrollbar: 		     true
!rofi.separator-style: 		     solid
rofi.columns: 			     1
rofi.fullscreen: 		     false
rofi.fake-transparency:		     true
rofi.theme: /home/kdani/repos/perfect-arch-config/templates/rofi-theme.rasi
